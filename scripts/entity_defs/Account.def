        </onEnterGameFailed>

        <!--进入游戏成功的回调-->
        <onEnterGameSuccess>
            <!--无参-->
        </onEnterGameSuccess>
        
        <!--FirstEntity hello的回调-->
        <onFirstEntityHello>
            <Arg>           UNICODE         </Arg>
        </onFirstEntityHello>
	</CellMethods>

</root>
