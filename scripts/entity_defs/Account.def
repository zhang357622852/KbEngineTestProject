<root>
	<Properties>
	</Properties>

	<ClientMethods>
	</ClientMethods>

	<BaseMethods>
        <createCell>
            <!--Scene的CELL ENTITYCALL-->
            <Arg>   ENTITYCALL      </Arg>
        </createCell>

        <!--暴露给客户端的进入游戏的方法-->
        <enterGame>
            <!--暴露给客户端的方法，需要声明该属性-->
            <Exposed/>
        </enterGame>
